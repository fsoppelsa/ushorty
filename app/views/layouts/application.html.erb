<!DOCTYPE html>
<html>
<head>
  <title>Ushorty</title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
  <%= yield :head %>
</head>
<body>
  
<h1 class="pagetitle"><%= t(:new_link) %> beta &mdash;
  <%= Rails.application.config.server_name[:host] %>
</h1>

<%= yield %>

<div id="disclaimer_title">
  <p>
  <%= t(:disclaimer_title) %>
  </p>
</div>

<div id="disclaimer">
  <p>
  <%= Rails.application.config.server_name[:host] %> <%= t(:disclaimer) %>
  </p>
</div>

<%= render 'shared/footer' %>
</body>
</html>
